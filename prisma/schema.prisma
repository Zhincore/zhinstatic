// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:cache.db?connection_limit=1"
}

model File {
  path String  @id
  mime String?
  ext  String?
}
